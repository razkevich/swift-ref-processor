    <replace_variables>Y</replace_variables>
    <quoteString>N</quoteString>
    <sql>insert into T&#x24;&#x7b;TABLES_SEQ_VAL1&#x7d;_SFS_COUNTRY&#xd;&#xa;SELECT new.&#x2a;&#xd;&#xa;FROM sfs_country old&#xd;&#xa;  INNER JOIN &#x28;&#xd;&#xa;&#xd;&#xa;               SELECT&#xd;&#xa;                 CASE WHEN isf.sepa &#x3d; &#x27;Y&#x27;&#xd;&#xa;                   THEN 1&#xd;&#xa;                 ELSE 0 END AS is_sepa,&#xd;&#xa;                 isf.iban_country_code,&#xd;&#xa;                 isf.bank_identifier_position,&#xd;&#xa;                 isf.iban_national_id_length,&#xd;&#xa;                 scc.country_name,&#xd;&#xa;                 isf.iban_total_length&#xd;&#xa;               FROM T&#x24;&#x7b;TABLES_SEQ_VAL1&#x7d;_sepa_ibanstructure isf&#xd;&#xa;                 JOIN T&#x24;&#x7b;TABLES_SEQ_VAL1&#x7d;_sepa_country_code scc ON isf.iban_country_code &#x3d; scc.country_code&#x29; new&#xd;&#xa;    ON new.IBAN_COUNTRY_CODE &#x3d; old.code&#xd;&#xa;       AND &#x28;NOT new.is_sepa &#x3d; old.IS_SEPA&#xd;&#xa;            OR NOT new.IBAN_COUNTRY_CODE &#x3d; old.CODE&#xd;&#xa;            OR NOT new.BANK_IDENTIFIER_POSITION &#x3d; old.START_FROM_SYMBOL&#xd;&#xa;            OR NOT new.IBAN_NATIONAL_ID_LENGTH &#x3d; old.NATIONAL_ID_LENGTH&#xd;&#xa;            OR NOT new.IBAN_TOTAL_LENGTH &#x3d; old.IBAN_TOTAL_LENGTH&#x29;</sql>
    <set_params>N</set_params>
