  <step>
    <name>calc_aggeregates&#x3a; calculate aggregates script &#x28;codebook&#x29; 2</name>
    <type>ExecSQL</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
