    <replace_variables>Y</replace_variables>
    <quoteString>N</quoteString>
    <sql>INSERT INTO T&#x24;&#x7b;TABLES_SEQ_VAL1&#x7d;_sepa_codebook&#xd;&#xa;  SELECT&#xd;&#xa;    rownum                    AS id,&#xd;&#xa;    ipf.iban_iso_country_code AS country_code,&#xd;&#xa;    ipf.iban_national_id      AS national_id,&#xd;&#xa;    CASE WHEN ipf.service_context &#x3d; &#x27;SEPA&#x27;&#xd;&#xa;      THEN 1&#xd;&#xa;    ELSE 0 END                AS isSepa,&#xd;&#xa;    CASE WHEN sdp.service_id &#x3d; &#x27;TARGET2&#x27;&#xd;&#xa;      THEN 1&#xd;&#xa;    ELSE 0 END                AS isTarget2,&#xd;&#xa;    ipf.iban_bic              AS BIC,&#xd;&#xa;    ipf.institution_name      AS bankName,&#xd;&#xa;    bdp.city                  AS bankCity,&#xd;&#xa;    ipf.country_name          AS bankCountry,&#xd;&#xa;    bdp.street_address_1      AS bankAddress,&#xd;&#xa;    bdp.zip_code              AS zipCode&#xd;&#xa;  FROM T&#x24;&#x7b;TABLES_SEQ_VAL1&#x7d;_sepa_ibanplus_v3 ipf&#xd;&#xa;    LEFT JOIN T&#x24;&#x7b;TABLES_SEQ_VAL1&#x7d;_sepa_bankdirectoryplus_v3 bdp ON ipf.iban_bic &#x3d; bdp.bic&#xd;&#xa;    LEFT JOIN &#x28;SELECT&#xd;&#xa;                 participant_id,&#xd;&#xa;                 service_id&#xd;&#xa;               FROM T&#x24;&#x7b;TABLES_SEQ_VAL1&#x7d;_sepa_servicedirectory_v1&#xd;&#xa;               WHERE service_id &#x3d; &#x27;TARGET2&#x27;&#xd;&#xa;               GROUP BY participant_id, service_id&#x29; sdp ON ipf.iban_bic &#x3d; sdp.participant_id&#xd;&#xa;  ORDER BY ipf.iban_iso_country_code, ipf.iban_national_id&#x3b;&#xd;&#xa;&#xd;&#xa;COMMIT&#x3b;&#xd;&#xa;</sql>
    <set_params>N</set_params>
    <insert_field/>
    <update_field/>
    <delete_field/>
    <read_field/>
    <arguments>
       <argument><name>TABLES_SEQ_VAL1</name></argument>
    </arguments>
